{
  "name": "cowegis/cowegis-api-bundle",
  "description": "Cowegis API Bundle - Content Web GIS API Bundle",
  "license": "LGPL-3.0-or-later",
  "type": "symfony-bundle",
  "keywords": [
    ""
  ],
  "authors": [
    {
      "name": "David Molineus",
      "email": "david.molineus@netzmacht.de"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "ext-pdo": "*",
    "beberlei/assert": "^3.2",
    "cowegis/cowegis-core": "^1.0",
    "goldspecdigital/oooas": "^2.8",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0",
    "symfony/config": "^5.4 || ^6.0 || ^7.0",
    "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
    "symfony/event-dispatcher-contracts": "^1.1 || ^2.0 || ^3.0",
    "symfony/http-foundation": "^5.4 || ^6.0 || ^7.0",
    "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
    "symfony/routing": "^5.4 || ^6.0 || ^7.0",
    "twig/twig": "^2.7 || ^3.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "^12.0",
    "netzmacht/phpspec-phpcq-plugin": "@dev",
    "phpcq/runner-bootstrap": "@dev",
    "phpspec/phpspec": "^7.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Cowegis\\Bundle\\Api\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Cowegis\\Bundle\\Api\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
